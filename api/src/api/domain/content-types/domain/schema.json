{
  "kind": "collectionType",
  "collectionName": "domains",
  "info": {
    "singularName": "domain",
    "pluralName": "domains",
    "displayName": "Domain"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "TLD": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "registration": {
      "type": "date",
      "required": true
    },
    "expiration": {
      "type": "date",
      "required": true
    },
    "ID": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::cart.cart",
      "mappedBy": "domain_id"
    }
  }
}
